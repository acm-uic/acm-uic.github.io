{
  "name": "acm-uic-github-io",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,json}\"",
    "prettier:ci": "prettier --check \"**/*.{js,jsx,ts,tsx,json}\"",
    "prettier:diff": "prettier --list-different \"**/*.{js,jsx,ts,tsx,json}\"",
    "prettier-md": "prettier --write \"**/*.{md,mdx}\"",
    "prettier-md:ci": "prettier --check \"**/*.{md,mdx}\"",
    "prettier-md:diff": "prettier --list-different \"**/*.{md,mdx}\"",
    "lint": "yarn lint:js && yarn lint:style && yarn lint:md",
    "lint:ci": "yarn lint:js --quiet && yarn lint:style && yarn lint:md",
    "lint:js": "eslint \"**/*.{js,jsx,ts,tsx}\"",
    "lint:md": "markdownlint **/*.md --ignore-path .eslintignore",
    "lint:style": "stylelint \"**/*.css\""
  },
  "dependencies": {
    "@docusaurus/core": "2.0.0-beta.6",
    "@docusaurus/preset-classic": "2.0.0-beta.8",
    "@mdx-js/react": "^1.6.22",
    "@svgr/webpack": "^5.5.0",
    "clsx": "^1.1.1",
    "file-loader": "^6.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-linkify": "^1.0.0-alpha",
    "swr": "^1.0.1",
    "url-loader": "^4.1.1"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^2.0.0-beta.0",
    "@tsconfig/docusaurus": "^1.0.2",
    "@types/eslint": "^7.28.0",
    "@types/gapi": "^0.0.41",
    "@types/gapi.client.calendar": "^3.0.10",
    "@types/react": "^17.0.6",
    "@types/react-helmet": "^6.1.1",
    "@types/react-linkify": "^1.0.1",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "markdownlint": "^0.24.0",
    "markdownlint-cli": "^0.28.1",
    "prettier": "^2.3.2",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "typescript": "^4.2.4"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
